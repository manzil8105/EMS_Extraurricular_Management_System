<VBox alignment="CENTER" prefHeight="720.0" prefWidth="1280.0" spacing="10.0" style="-fx-background-color: #181f2b;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectmain.FileStorageController">
    <children>
      <Pane prefHeight="389.0" prefWidth="1280.0">
         <children>
              <Button fx:id="chooseFileButton" layoutX="567.0" layoutY="119.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="130.0" stylesheets="@../../../CSS/Button.css" text="Upload File">
               <font>
                  <Font name="Bernard MT Condensed" size="12.0" />
               </font>
            </Button>
              <Button fx:id="downloadButton" layoutX="568.0" layoutY="188.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="130.0" stylesheets="@../../../CSS/Button.css" text="Download File">
               <font>
                  <Font name="Bernard MT Condensed" size="12.0" />
               </font>
            </Button>
              <Button fx:id="refreshButton" layoutX="568.0" layoutY="253.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="130.0" stylesheets="@../../../CSS/Button.css" text="Refresh List">
               <font>
                  <Font name="Bernard MT Condensed" size="12.0" />
               </font>
            </Button>
            <Button fx:id="backButton" layoutX="24.0" layoutY="14.0" mnemonicParsing="false" onAction="#backOnAction" prefHeight="53.0" prefWidth="54.0" stylesheets="@../../../CSS/Button.css" text="&lt;" />
            <Label alignment="CENTER" layoutX="523.0" layoutY="20.0" prefHeight="53.0" prefWidth="219.0" text="PRIVATE FILES" textFill="#f7a100">
               <font>
                  <Font size="30.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <TextArea fx:id="messageTextField" prefHeight="37.0" prefWidth="449.0" />
        <ListView fx:id="fileListView" prefHeight="335.0" prefWidth="538.0" style="-fx-background-color: #181f2b;" />
    </children>
</VBox>
